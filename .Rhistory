zoom_fig@xmin = zoom_fig@xmin - 300
zoom_fig@xmax = zoom_fig@xmax + 300
zoom_fig@ymin = zoom_fig@ymin - 300
zoom_fig@ymax = zoom_fig@ymax + 300
ffzoom_cl = raster::crop(fire_frequency, zoom_fig)
